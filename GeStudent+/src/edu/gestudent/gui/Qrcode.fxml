<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="510.0" prefWidth="550.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.gestudent.gui.QrcodeController">
   <children>
      <VBox layoutX="10.0" layoutY="10.0" prefHeight="449.0" prefWidth="557.0" spacing="40.0" AnchorPane.bottomAnchor="30.0" AnchorPane.leftAnchor="40.0" AnchorPane.rightAnchor="40.0" AnchorPane.topAnchor="30.0">
         <children>
            <JFXTextField fx:id="txtQrCode" prefHeight="25.0" prefWidth="195.0" promptText="Enter your Qr Code">
               <VBox.margin>
                  <Insets right="20.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="txtusername" prefHeight="25.0" prefWidth="195.0" promptText="User Name">
               <VBox.margin>
                  <Insets right="20.0" />
               </VBox.margin>
            </JFXTextField>
            <HBox spacing="20.0">
               <children>
                  <JFXTextField fx:id="txtimage" prefHeight="37.0" prefWidth="231.0" promptText="Image" />
                  <JFXButton fx:id="uploadbutton" buttonType="RAISED" onAction="#uploadimage" text="..." />
               </children>
            </HBox>
            <JFXPasswordField fx:id="txtpassword" promptText="password" />
            <JFXPasswordField fx:id="txtconformation" promptText="confirmation" />
            <JFXButton buttonType="RAISED" onAction="#createAcount" prefHeight="25.0" prefWidth="223.0" text="Creat Account">
               <VBox.margin>
                  <Insets left="150.0" />
               </VBox.margin>
            </JFXButton>
         </children>
      </VBox>
   </children>
</AnchorPane>
